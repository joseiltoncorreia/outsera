{
  "name": "testes-api-cypress",
  "version": "1.0.0",
  "description": "Testes automatizados de API usando Cypress",
  "main": "index.js",
  "scripts": {
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:headed": "cypress run --headed",
    "test:api": "cypress run --spec 'cypress/e2e/api-tests.cy.js'",
    "test:report": "cypress run --spec 'cypress/e2e/api-tests.cy.js' --reporter mochawesome",
    "open-report": "node open-report.js"
  },
  "keywords": [
    "cypress",
    "api",
    "testing",
    "automation"
  ],
  "author": "Seu Nome",
  "license": "MIT",
  "devDependencies": {
    "cypress": "^13.6.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.2",
    "mochawesome-report-generator": "^6.2.0"
  }
}
